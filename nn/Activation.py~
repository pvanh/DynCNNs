
def dummyIdentity(dummy, x):
    return x
def dummyEye(dummy, x):
    return np.ones_like(x)


def dummySigmoid(dummy, x):
    #e = 2.718281828459045
    import numpy as np

    return 1/ (1 + np.e ** (-x) )

def sigmoid(x):
    return 1 / (1 + np.e ** -x )
    
def dummySigmoidPrime(dummy, y):

    return y * (1 - y)

